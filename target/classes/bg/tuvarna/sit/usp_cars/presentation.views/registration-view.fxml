<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="500.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Pane prefHeight="500.0" prefWidth="373.0" styleClass="leftMenu">
            <children>
            </children>
        </Pane>
        <Pane layoutX="373.0" maxHeight="500.0" maxWidth="613.0" minHeight="500.0" minWidth="373.0" prefHeight="500.0" prefWidth="373.0" styleClass="inputField">
            <children>
                <Label layoutX="75.0" layoutY="92.0" styleClass="labelTitle" text="Please enter your username and password:" textAlignment="CENTER" wrapText="true" />
                <TextField fx:id="user_username" layoutX="87.0" layoutY="162.0" prefHeight="45.0" prefWidth="200.0" styleClass="inputFields" />
                <PasswordField fx:id="user_password" layoutX="87.0" layoutY="250.0" prefHeight="45.0" prefWidth="200.0" styleClass="inputFields" />
                <Button fx:id="login" layoutX="103.0" layoutY="337.0" mnemonicParsing="false" onAction="#onRegistrationButtonClick" prefHeight="55.0" prefWidth="167.0" styleClass="confirmButtons" text="Register" wrapText="true">
                    <font>
                        <Font size="10.0" />
                    </font></Button>
                <Label layoutX="90.0" layoutY="145.0" text="Username" />
                <Label layoutX="92.0" layoutY="223.0" text="Password" />
            <Button styleClass="emptyButton" layoutX="104.0" layoutY="399.0" onAction="#goBack" mnemonicParsing="false" prefHeight="25.0" prefWidth="167.0" text="Already have an account?" />
            <Label layoutX="123.0" layoutY="40.0" text="Registration" textAlignment="CENTER">
               <font>
                  <Font size="24.0" />
               </font></Label>
            </children>
        </Pane>
    </children>
</AnchorPane>
